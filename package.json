{
  "name": "reading-room-engine",
  "version": "0.1.0",
  "private": false,
  "description": "The Reading Room Engine â€“ automated queue and invitation system for weekly community reading sessions in Ahmedabad.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marmik-Soni/ReadingRoom.git"
  },
  "bugs": {
    "url": "https://github.com/Marmik-Soni/ReadingRoom/issues"
  },
  "homepage": "https://readingroom.vercel.app",
  "keywords": [
